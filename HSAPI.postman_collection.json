		{
			"name": "Auth",
			"item": [
				{
					"name": "Login_CSP",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "93a8a484-fc88-425a-9011-8acf73804020",
								"exec": [
									"// Parse the response to extract the value of the csp id_token to a variable cspTokenId.",
									"var response = JSON.parse(responseBody);",
									"pm.environment.set(\"cspTokenId\", response.id_token);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"type": "text",
								"value": "application/x-www-form-urlencoded"
							}
						],
						"body": {
							"mode": "urlencoded",
							"urlencoded": [
								{
									"key": "grant_type",
									"value": "refresh_token",
									"type": "text"
								},
								{
									"key": "refresh_token",
									"value": "{{cspUserRefreshToken}}",
									"type": "text"
								}
							]
						},
						"url": {
							"raw": "https://{{cspAddress}}/csp/gateway/am/api/auth/api-tokens/authorize",
							"protocol": "https",
							"host": [
								"{{cspAddress}}"
							],
							"path": [
								"csp",
								"gateway",
								"am",
								"api",
								"auth",
								"api-tokens",
								"authorize"
							]
						},
						"description": "This API allows the user to exchange an organization scoped API-token for a user access token."
					},
					"response": []
				},
				{
					"name": "Hydra_Local_login",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "45ff4343-567d-4188-bd05-7760b9c86ed8",
								"exec": [
									"// Parse the response to extract the value of the hydra apiToken to a variable hydraApiToken.",
									"var response = JSON.parse(responseBody);",
									"pm.environment.set(\"hsApiToken\", response.apiToken);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n      \"credentialRequested\": \"cspToken\",\n      \"credentialToken\": \"{{cspTokenId}}\"\n}"
						},
						"url": {
							"raw": "https://{{hydraAddress}}/api/login/login",
							"protocol": "https",
							"host": [
								"{{hydraAddress}}"
							],
							"path": [
								"api",
								"login",
								"login"
							]
						},
						"description": "Use the CSP User Access Token to fetch a Hydra API Token that will be used for subsequent HSAPI invocations."
					},
					"response": []
				}
			]
		},
		{
			"name": "Pods",
			"item": [
				{
					"name": "Get_Pods",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "02e8cecb-4413-474c-8bdd-60d05565c9cd",
								"exec": [
									"//Initialize a local variable with the name of the Pod.",
									"var pod = pm.variables.get(\"podName\");",
									"",
									"//Capture the response body in a variable for parsing later.",
									"var response = JSON.parse(responseBody);",
									"",
									"//Traverse the response to fetch the id value for the pod specified in the environment variables.",
									"for(var i=0; i < response.values.length; i++) {",
									"    if(response.values[i].name==pod) {",
									"        pm.environment.set(\"podId\", response.values[i].id);",
									"    }",
									"}",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
